<?xml version="1.0"?>

<launch>
  <include
    file="$(find gazebo_ros)/launch/empty_world.launch" >
    <arg name="world_name" value="$(find simulation_watchdog)/worlds/small_maze.world"/>
  </include>
  <node
    name="tf_base_footprint_base_link"
    pkg="tf"
    type="static_transform_publisher"
    args="0 0 -0.0509 0 0 0 base_link base_footprint 100" />
  <node
    name="spawn_model"
    pkg="gazebo_ros"
    type="spawn_model"
    args="-file $(find simulation_watchdog)/urdf/watchdog.urdf -urdf -model watchdog"
    output="screen" />
  <node
    name="fake_joint_calibration"
    pkg="rostopic"
    type="rostopic"
    args="pub /calibrated std_msgs/Bool true" />
</launch>
